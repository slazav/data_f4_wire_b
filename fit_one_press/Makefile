WIRES = w1a w1b w2a w1bt w2bt
TRG = $(patsubst %,p00_%,${WIRES})\
      $(patsubst %,p02_%,${WIRES})\
      $(patsubst %,p05_%,${WIRES})

all: $(patsubst %,%.res3, ${TRG})\
     $(patsubst %,%.png, ${TRG})
	echo $(patsubst %,%.res3, ${TRG})


W:=w1a

p00_${W}.res3 p00_${W}.png:\
 ../20230510-00bar-136mT/${W}.res\
 ../20230512-00bar-113mT/${W}.res\
 ../20230513-00bar-091mT/${W}.res\
 ../20230515-00bar-069mT/${W}.res\
 ../20230516-00bar-046mT/${W}.res
	./proc3 p00 $^

p02_${W}.res3 p02_${W}.png:\
 ../20230425-02bar-158mT/${W}.res\
 ../20230427-02bar-136mT/${W}.res\
 ../20230429-02bar-113mT/${W}.res\
 ../20230430-02bar-091mT/${W}.res\
 ../20230501-02bar-069mT/${W}.res\
 ../20230502-02bar-046mT/${W}.res
	./proc3 p02 $^

p05_${W}.res3 p05_${W}.png:\
 ../20230525-05bar-203mT/${W}.res\
 ../20230527-05bar-158mT/${W}.res\
 ../20230528-05bar-136mT/${W}.res\
 ../20230530-05bar-091mT/${W}.res\
